{
  "name": "gimlyid-qr-code",
  "version": "0.1.0",
  "description": "A QR component for Gimly ID",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "run-s build test:* cov",
    "test:lint": "eslint . --ext .ts",
    "test:prettier": "prettier \"{src, test}/**/*.(ts|tsx)\" --list-different",
    "test:unit": "jest",
    "cov": "jest --coverage",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"{src, test}/**/*.(ts|tsx)\" --write",
    "fix:lint": "eslint . --ext .ts --fix"
  },
  "files": [
    "/dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "react": "^17.0.2",
    "react-qr-code": "^2.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "chai": "^4.3.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.24.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "react-dom": "^17.0.2",
    "typescript": "^4.4.3",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  }
}
